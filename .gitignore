# Ignore directories generated by package managers
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/
/web/*.*
/node_modules/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.*.php

# Ignore Drupal's file directories
/web/sites/*/files/
/private/files/*
!/private/files/.gitkeep
/private/keys/*
!/private/keys/.gitkeep

# Ignore OSX indecies
.DS_Store

# Packages #
############
*.7z
*.dmg
*.gz
*.bz2
*.iso
*.jar
*.rar
*.tar
*.zip
*.tgz

# Logs and databases #
######################
*.log
*.sql